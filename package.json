{
  "name": "art-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "update:types": "npm update @art-vbst/art-types",
    "release:patch": "npm version patch -m 'chore(release): patch %s'",
    "release:minor": "npm version minor -m 'chore(release): minor %s'",
    "release:major": "npm version major -m 'chore(release): major %s'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@art-vbst/art-types": "^1.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.3",
    "vite": "^7.1.9"
  }
}
